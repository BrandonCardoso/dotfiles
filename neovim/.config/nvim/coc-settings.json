{
  "tsserver.log": "off",
  "tsserver.trace.server": "off",
  "diagnostic.checkCurrentLine": true,
  "diagnostics.displayByAle": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "go"
  ],
  "coc.proferences.hoverTarget": "float",
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod"],
      "filetypes": ["go"]
    }
  }
  "go.goplsOptions": {
    "staticcheck": true
  },
  "snippets.ultisnips.pythonPrompt": false
}
