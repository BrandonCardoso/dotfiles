[submodule ".vim/bundle/Vundle.vim"]
	path = vim/.vim/bundle/Vundle.vim
	url = https://github.com/VundleVim/Vundle.vim.git
[submodule "neovim/.config/nvim/vim-plug"]
	path = neovim/.config/nvim/vim-plug
	url = https://github.com/junegunn/vim-plug
[submodule "polybar/.config/polybar/scripts/polybar-pulseaudio-control"]
	path = polybar/.config/polybar/scripts/polybar-pulseaudio-control
	url = https://github.com/marioortizmanero/polybar-pulseaudio-control.git
[submodule "fcitx/.config/fcitx/skin/fcitx-dracula"]
	path = fcitx/.config/fcitx/skin/fcitx-dracula
	url = https://github.com/BrandonCardoso/fcitx-dracula.git
	branch = main
[submodule "alacritty/.config/alacritty/colors/dracula"]
	path = alacritty/.config/alacritty/colors/dracula
	url = git@github.com:dracula/alacritty.git
[submodule "shell/.tmux/plugins/tpm"]
	path = shell/.tmux/plugins/tpm
	url = https://github.com/tmux-plugins/tpm
